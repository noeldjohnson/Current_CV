@article{JohnsonEtAl11,
	Abstract = {{We estimate the impact of corruption on growth of output per worker in
   U.S. states. We improve on existing studies of the cost of corruption
   by using a better specified empirical model, focusing on a study
   population that is less likely to be affected by parameter
   heterogeneity, and controlling for endogeneity using political
   variables to instrument for corruption. We find that corruption plays a
   significant and causal role in lowering growth and investment across
   the states.}},
	Author = {Johnson, Noel D. and Courtney L. Lafountain and Steven Yamarik},
	Date-Added = {2011-07-16 17:20:18 -0400},
