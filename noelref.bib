@article{Ma03,
	Abstract = {The last two decades of the 20th century witnessed a tide of privatization of public services under the banner of New Public Management (NPM). Interestingly, some proponents and opponents of NPM assume that NPM reform is something genuinely new. Drawing from administrative history during the early modern period (1500-1800), this paper argues that NPM is not genuinely new; instead, there were uncelebrated forerunners in the early modern period. From a detailed analysis of tax farming in the early modern period, this paper offers three findings that are useful for assessing the strengths and weaknesses of NPM reform, especially its privatization strategy. They are: 1. although tax farming was used in early times for its relative efficiency, it was not a cost-free mechanism and its short-term efficiency would have detrimental long-term results in revenue generation and financial management; 2. because of privatization, the public interest might be undermined by private profits; and 3. with privatization, citizens would be subject to the greed of tax farmers. These lessons have implications for contemporary NPM reform, especially its privatization strategy, leading to a warning that what had occurred under the tax farming system may happen with the recent privatization of public services.},
	Author = {Ma, Jun},
	Copyright = {Copyright {\copyright} 2003 M.E. Sharpe, Inc.},
	Date-Added = {2012-07-10 13:45:38 +0100},
	Date-Modified = {2012-07-10 13:45:46 +0100},
	Issn = {10841806},
